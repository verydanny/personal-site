{
  "compilerOptions": {
    "target": "es2018",
    "moduleResolution": "node",
    "importHelpers": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "outDir": "lib/",
    "downlevelIteration": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
    "strictFunctionTypes": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom"
    ]
  }
}
