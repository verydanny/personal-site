{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "preserve",
    "importHelpers": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "removeComments": true,
    "noEmitHelpers": true,
    "noLib": false,
    "outDir": "lib/",
    "preserveConstEnums": true,
    "downlevelIteration": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"strictFunctionTypes": false
  }
}
